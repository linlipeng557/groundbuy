<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--自定义标题栏-->
    <declare-styleable name="CustomTitleBar">

        <attr name="immersiv" format="boolean" />
        <attr name="titleBarHeight" format="dimension" />

        <attr name="centerTitleSize" format="dimension" />
        <attr name="rightTitleSize" format="dimension" />
        <attr name="leftTitleSize" format="dimension" />

        <attr name="leftTitle" format="string" />
        <attr name="centerTitle" format="string" />
        <attr name="rightTitle" format="string" />

        <attr name="leftTitleColor" format="color" />
        <attr name="centerTitleColor" format="color" />
        <attr name="rightTitleColor" format="color" />

        <attr name="leftImage" format="reference" />
        <attr name="rightImage" format="reference" />
        <attr name="rightSecondImage" format="reference" />

        <attr name="dividerShow" format="boolean" />
        <attr name="dividerColor" format="color" />
        <attr name="backgroundColor" format="color" />

    </declare-styleable>


</resources>